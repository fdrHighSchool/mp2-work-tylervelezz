public class Main {
  public static void main(String[] args) {
    System.out.println("FizzBuzz Generator");
    for(int num = 0; num<101; num++){

      if (num % 3==0 && num % 5==0){
        System.out.println("FizzBuzz");
        }

       else if (num % 5 ==0){
         System.out.println("buzz");
       }
        else if(num % 3 ==0){
          System.out.println("fizz");
        }
        else{
          System.out.println(num);
        }
       
      
